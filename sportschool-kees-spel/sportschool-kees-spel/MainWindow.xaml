<Window x:Class="sportschool_kees_spel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sportschool_kees_spel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#6d92ac">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="609*"/>
            <ColumnDefinition Width="191*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Height="200" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Button Content="Page1" MinWidth="100" MinHeight="40" Click="BtnClickCSW" Background="#FFF3C9A6"/>
            <Button Content="Page2" MinWidth="100" MinHeight="40" Margin="0,10,0,0" Click="BtnClickCSW2" Background="#FFF3C9A6"/>
            <Button Content="Page3" MinWidth="100" MinHeight="40" Margin="0,10,0,0" Click="BtnClickCSW2" Background="#FFF3C9A6"/>
            <Button Content="Page4" MinWidth="100" MinHeight="40" Margin="0,10,0,0" Click="BtnClickCSW2" Background="#FFF3C9A6"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="100,0,0,0" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2" Navigated="Main_Navigated" Background="#586c70"/>
    </Grid>
</Window>
